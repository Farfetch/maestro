    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run: client Agent",
            "type": "python",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "env": {
                "JMETER_CONTAINER_NAME": "maestrojmeter-client",
                "AGENT_HOST": "client.maestro.net"
            }
        },
        {
            "name": "Run: Server Agent",
            "type": "python",
            "request": "launch",
            "program": "run.py",
            "console": "integratedTerminal",
            "env": {
                "JMETER_CONTAINER_NAME": "maestrojmeter-server1",
                "AGENT_HOST": "server1.maestro.net"
            }
        }
    ]
}
